<def name="Tag Game">
	<element name="spawnpoint" friendlyname="Spawnpoint">
		<data name="position" type="coord3d" default="0,0,0" />
		<data name="rotation" type="number" default="0" />
		<data name="skin" type="skinID" default="0" />
		<ped model="!skin!" position="!position!" rotZ="!rotation!" />
	</element>
	<element name="vehicleSpawn" friendlyname="Vehicle Spawn">
		<data name="position" type="coord3d" default="0,0,0" />
		<data name="rotation" type="number" default="0" />
		<vehicle model="507" position="!position!" />
	</element>
</def>